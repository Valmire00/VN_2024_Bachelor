{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "overrides": [
      {
        "files": ["*.json"],
        "parser": "json-eslint-parser"
      }
    ],
    "rules": {
      "quotes": ["error", "double"]
    }
  }
  